package com.train.day2.primitivetypes;

public class TypeConversion {

	public static void main(String[] args) {
		int a; //declaration
		a = 20; // initialization
		int b = 10; // both declaration and initialization 
		System.out.println(a+b);
		int c = 30;
		int d = 40;
		System.out.println(c + d);
	}
}
