package com.train.week1day2.qn04;

public class StringDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String org = "Testes";
		//org = org + " leaf";
		System.out.println(org);
		String org1 = new String("Test");
		System.out.println(org == org1);
		System.out.println(org.equals(org1));
		String org3 = "Test";
		System.out.println(org == org3);
		System.out.println(org.length());
		System.out.println(org.toUpperCase());
		System.out.println(org.charAt(1));
		System.out.println(org.indexOf('t'));
		System.out.println(org.indexOf('j'));
		System.out.println(org.concat(" Leaf"));
		System.out.println(org.replace('t', 'z'));
		System.out.println(org.repeat(1));
		System.out.println(org.toCharArray());
	
		String age = "23";
		int valueAge = Integer.parseInt(age);
		System.out.println(valueAge);
		
		int age1 = 23;
		String valueAge1 = String.valueOf(age1);
		System.out.println(valueAge1);
		
		float age2 = 23;
		String valueAge2 = String.valueOf(age2);
		System.out.println(valueAge2);
		
	}

}
